<form method="post" class="form-delete" action="{{ path(entityName~'_delete', {'id': register.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ register.id) }}">
    <button class="btn btn-danger">Delete</button>
</form>
