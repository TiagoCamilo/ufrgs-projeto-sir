<table class="table">
    <thead>
    <tr>
        {% for key, attr in registerListAttr|first %}
            <th>{{ key }}</th>
        {% endfor %}
        <th>Ações</th>
    </tr>
    </thead>
    <tbody>
    {% for key, listAttr in registerListAttr %}
        <tr>
            {% for attr in listAttr %}
                {#TODO: Aplicar raw somente no atributo correto#}
                <td>{{ attr | raw}}</td>
            {% endfor %}
            <td>
                {% if(actionList.show == true) %}
                    <a class="btn btn-primary" href="{{ path(entityName~'_report_pdf', {'id': listAttr.Id}) }}">PDF</a>
                {% endif %}
                {% if(actionList.image == true) %}
                    <a class="btn btn-primary" href="{{ path(entityName~'_image', {'id': listAttr.Id}) }}">Foto</a>
                {% endif %}
                {% if(actionList.show == true) %}
                    <a class="btn btn-primary" href="{{ path(entityName~'_show', {'id': listAttr.Id}) }}">Exibir</a>
                {% endif %}
                {% if(actionList.edit == true) %}
                    <a class="btn btn-primary" href="{{ path(entityName~'_edit', {'id': listAttr.Id}) }}">Editar</a>
                {% endif %}
                {% if(actionList.delete == true) %}
                    {%  include template.delete with {'register': {'id': listAttr.Id}} %}
                {% endif %}
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>
